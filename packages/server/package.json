{
	"name": "@http-rpc/server",
	"version": "0.5.2",
	"description": "HTTP RPC Server",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		".": "./dist/index.js",
		"./adapters/*": "./dist/adapters/*.js"
	},
	"typesVersions": {
		"*": {
			"adapters/*": [
				"dist/adapters/*.d.ts"
			]
		}
	},
	"scripts": {
		"build": "rm -rf dist && tsup src/index.ts src/adapters/*.ts --format esm --dts",
		"build:watch": "npm run build -- --watch",
		"lint": "tsc",
		"prepublishOnly": "npm run build",
		"test-d": "tsd"
	},
	"keywords": [
		"http",
		"rpc",
		"server",
		"axios",
		"http-rpc",
		"http-rpc-server",
		"fastify",
		"express",
		"koa",
		"hapi",
		"rest",
		"typescript",
		"api",
		"json",
		"rpc",
		"http-rpc-server"
	],
	"author": "LacheRo`",
	"license": "MIT",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lacherogwu/http-rpc.git",
		"directory": "packages/server"
	},
	"dependencies": {
		"fastify-plugin": "^5.0.1",
		"zod": "^4.0.17"
	},
	"devDependencies": {
		"fastify": "^5.5.0"
	}
}
